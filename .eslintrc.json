{
  "extends": "airbnb-base",
  "parserOptions": {
      "sourceType": "script"
  },
  "env":{
      "browser": true
  },
  "rules": {
      "no-param-reassign": [ "error", { "props": false } ],
      "no-restricted-globals": "off",
      "padded-blocks": [ "error", "always" ],
      "space-before-function-paren": "off",
      "strict": [ "error", "function" ]
  }
}
